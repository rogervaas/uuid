{
  "name": "uuid",
  "version": "3.4.0",
  "description": "RFC4122 (v1, v4, and v5) UUIDs",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "uuid",
    "guid",
    "rfc4122"
  ],
  "license": "MIT",
  "bin": {
    "uuid": "./bin/uuid"
  },
  "sideEffects": false,
  "main": "index.js",
  "module": "esm-browser/index.js",
  "devDependencies": {
    "@babel/cli": "~7.6.4",
    "@babel/core": "~7.6.4",
    "@babel/preset-env": "~7.6.3",
    "@commitlint/cli": "~8.2.0",
    "@commitlint/config-conventional": "~8.2.0",
    "babel-eslint": "~10.0.3",
    "babel-plugin-add-module-exports": "~1.0.2",
    "browserstack-local": "~1.4.2",
    "eslint": "~6.4.0",
    "eslint-config-prettier": "~6.4.0",
    "eslint-plugin-prettier": "~3.1.1",
    "esm": "~3.2.25",
    "http-server": "~0.12.0",
    "husky": "~3.0.9",
    "jest": "~24.9.0",
    "lint-staged": "~9.4.3",
    "npm-run-all": "~4.1.5",
    "prettier": "~1.18.2",
    "runmd": "1.2.1",
    "selenium-webdriver": "~3.6.0",
    "standard-version": "7.0.0"
  },
  "scripts": {
    "examples:browser-webpack:build": "cd examples/browser-webpack && yarn install && yarn run build",
    "examples:browser-rollup:build": "cd examples/browser-rollup && yarn install && yarn run build",
    "examples:browser-esmodules:build": "cd examples/browser-esmodules && yarn install && yarn run build",
    "lint": "yarn run eslint:check && yarn run prettier:check",
    "eslint:check": "eslint src/ test/ examples/ *.js",
    "eslint:fix": "eslint --fix src/ test/ examples/ *.js",
    "test": "BABEL_ENV=commonjs jest test/unit/",
    "pretest:browser": "yarn run build && npm-run-all --parallel examples:**",
    "test:browser": "BABEL_ENV=commonjs jest --forceExit --verbose test/browser/${BROWSER:-}*",
    "prettier:check": "prettier --ignore-path .prettierignore --check '**/*.{js,jsx,json,md}'",
    "prettier:fix": "prettier --ignore-path .prettierignore --write '**/*.{js,jsx,json,md}'",
    "ci": "yarn run lint && yarn run test && yarn run prettier:check && yarn run docs:diff",
    "md": "runmd --watch --output=README.md README_js.md",
    "docs": "( node --version | grep -q 'v12' ) && ( yarn run build && runmd --output=README.md README_js.md )",
    "docs:diff": "( node --version | grep -vq 'v12' ) || ( yarn run docs && git diff --quiet README.md )",
    "build": "./scripts/build.sh",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uuidjs/uuid.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
